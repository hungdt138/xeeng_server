package com.tv.xeeng.game.newpika.data;

import com.tv.xeeng.base.room.NRoomEntity;

/**
 *
 * @author tuanda
 */
public class Topic {
    public String name;
    public int id;
    public String detail;// link
    public String icon;
    public long money;
    public int roomID;
    public NRoomEntity room;
    public Topic(String name, int id, String detail, String icon, long money, int roomID) {
        this.name = name;
        this.id = id;
        this.detail = detail;
        this.icon = icon;
        this.money = money;
        this.roomID = roomID;
    }  
    
}
