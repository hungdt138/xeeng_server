package com.tv.xeeng.game.bet;

public class EuroBetEntity {
	public double chuThang;
	public double hoa;
	public double khachThang;
	public EuroBetEntity(double chuThang, double hoa, double khachThang) {
		this.chuThang = chuThang;
		this.hoa = hoa;
		this.khachThang = khachThang;
	}
	
	public String toString(){
		String res = "";
		res += chuThang+":";
		res += hoa+":";
		res += khachThang;
		return res;
	}
}
